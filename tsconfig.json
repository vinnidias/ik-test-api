{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
  
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": false,
    "noUnusedLocals": false,
    "lib": ["ES2018"],
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": true,
    "moduleResolution": "node",
  
  }
}
